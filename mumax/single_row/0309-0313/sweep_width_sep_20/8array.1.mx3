s:= 20e-9

rect1 := Cuboid(b, a, z).transl(-(0.5*s + 0.5*b),0,0)
rect2 := Cuboid(b, a, z).transl(0.5*s + 0.5*b,0,0)

DefRegion(1, rect1)
DefRegion(2, rect2)

Msat.SetRegion(1, Ms)
m.SetRegion(1, uniform(0,-1,0))
